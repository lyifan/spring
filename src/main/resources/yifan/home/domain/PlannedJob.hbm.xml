<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.isotrak.hibernate.domain">
   <class name="PlannedJob" table="PlannedJob">
        <id name="id" column="PlannedJobID">
            <generator class="native"/>
        </id>
        <property name="start" type="timestamp" column="StartTime"/>
        <property name="end" type="timestamp" column="EndTime"/>
        <property name="reference" column="JobReference"/>
    </class>
</hibernate-mapping>
